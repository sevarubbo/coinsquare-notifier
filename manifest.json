{
  "name": "Coinsquare notifier",
  "description" : "Adds sound notifications for coinsquare web app",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://coinsquare.com/trade/*"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
